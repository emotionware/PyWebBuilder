select cast( IDPRODUCTIVIDAD as UNSIGNED) as  IDPRODUCTIVIDAD ,IDSYSUSER,USERNAME,ACCION,TABLA,CONVERT_TZ(FECHA,'+00:00','+06:00') as FECHA ,HORA,AUXILIAR1_char,AUXILIAR2_char,AUXILIAR1_Memo,AUXILIAR2_Memo,AUXILIAR1_Check,AUXILIAR2_Check,CONVERT_TZ(AUXILIAR1_date,'+00:00','+06:00') as AUXILIAR1_date ,AUXILIAR1_time,AUXILIAR1_decimal,AUXILIAR2_decimal,OBSERVACIONES,ELIMINADO,CREADOR,BLOQUEADO,GUID,PROPIETARIO,CAMPODISPLAY,CONVERT_TZ(FECHAALTA,'+00:00','+06:00') as FECHAALTA ,CONVERT_TZ(FECHAMODIFICACION,'+00:00','+06:00') as FECHAMODIFICACION ,HORAMODIFICACION,IMAGENADJUNTA,ULTIMOUSUARIO,IDESTATUS_tblproductividad, concat('<a style="color:blue;" href="javascript:modoedicion(',IDPRODUCTIVIDAD,');">Editar</a>') as EDITAR from tblproductividad  order by IDPRODUCTIVIDAD desc